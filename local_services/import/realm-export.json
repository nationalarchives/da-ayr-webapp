{
  "realm": "tdr",
  "enabled": true,
  "sslRequired": "none",
  "groups": [
    {
      "name": "ayr_user_type",
      "path": "/ayr_user_type",
      "subGroups": [
        {
          "name": "download",
          "path": "/ayr_user_type/download"
        },
        {
          "name": "view_all",
          "path": "/ayr_user_type/view_all"
        },
        {
          "name": "view_dept",
          "path": "/ayr_user_type/view_dept"
        }
      ]
    },
    {
      "name": "transferring_body_user",
      "path": "/transferring_body_user",
      "subGroups": [
        {
          "name": "Testing A",
          "path": "/transferring_body_user/Testing A"
        }
      ]
    }
  ],
  "requiredCredentials": [
    "password"
  ],
  "clients": [
    {
      "clientId": "ayr-beta",
      "name": "ayr-beta",
      "description": "Access your records keycloak client",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "test-client-secret",
      "redirectUris": [
        "https://127.0.0.1:5000/*",
        "https://localhost:5000/*",
        "https://host.docker.internal:5000/*",
        "http://127.0.0.1:5000/*",
        "http://localhost:5000/*",
        "http://host.docker.internal:5000/*"
      ],
      "webOrigins": [
        "https://127.0.0.1:5000",
        "https://localhost:5000",
        "https://host.docker.internal:5000",
        "http://127.0.0.1:5000",
        "http://localhost:5000",
        "http://host.docker.internal:5000"
      ],
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": true,
      "frontchannelLogout": true,
      "protocol": "openid-connect",
      "attributes": {
        "use.refresh.tokens": "true",
        "backchannel.logout.session.required": "true"
      },
      "fullScopeAllowed": true,
      "defaultClientScopes": [
        "group_mapper_client_scope",
        "basic",
        "roles",
        "web-origins"
      ],
      "protocolMappers": [
        {
          "name": "audience-mapper",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-audience-mapper",
          "consentRequired": false,
          "config": {
            "included.client.audience": "ayr-beta",
            "id.token.claim": "false",
            "access.token.claim": "true",
            "userinfo.token.claim": "false"
          }
        }
      ]
    }
  ],

